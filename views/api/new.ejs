<%- include('../partials/header') %>
<form action="/api" method="POST">
  <div class="form-row">
      
            <div class="form-group col-md-6">
              <label for="inputEmail4">id</label>
              <input type="number" name="id" class="form-control" id="inputEmail4" placeholder="">
            </div>
      
            <div class="form-group col-md-6">
              <label for="inputEmail4">Name</label>
              <input type="text" name="name" class="form-control" id="inputEmail4" placeholder="Name">
            </div>

      <div class="form-group col-md-6">
        <label for="inputEmail4">Phone</label>
        <input type="tel" name="phone" class="form-control" id="inputEmail4" placeholder="123-123456">
      </div>

    </div>

    <div class="form-group">
      <label for="inputAddress">Address</label>
      <input type="text" name="address" class="form-control" id="inputAddress" placeholder="1234 Main St">
    </div>

    <div class="form-row">

      <div class="form-group col-md-6">
        <label for="inputCity">City</label>
        <input type="text" name="city" class="form-control" id="inputCity">
      </div>

      <div class="form-group col-md-6">
        <label for="inputCity">State</label>
        <input type="text" name="state" class="form-control" id="inputCity">
      </div>

      <div class="form-group col-md-6">
        <label for="inputCity">Country</label>
        <input type="text" name="country" class="form-control" id="inputCity">
      </div>

      <div class="form-group col-md-2">
        <label for="inputZip">Zip</label>
        <input type="text" name="postalZip" class="form-control" id="inputZip" placeholder="123456">
      </div>

      <div class="form-group col-md-2">
        <label for="inputZip">Email</label>
        <input type="email" name="email" class="form-control" id="inputZip" placeholder="example@example.com">
      </div>

      <div class="form-group col-md-2">
        <label for="inputZip">Company</label>
        <input type="text" name="Company" class="form-control" id="inputZip" placeholder="Company">
      </div>

      <div class="form-group col-md-2">
        <label for="inputZip">Company Funded</label>
        <input type="number" name="companyFunded" class="form-control" id="inputZip" placeholder="100">
      </div>

      <div class="form-group col-md-2">
        <label for="inputZip">Team</label>
        <input type="number" name="team" class="form-control" id="inputZip" placeholder="">
      </div>

      <div class="form-group col-md-2">
        <label for="inputZip">Month</label>
        <input type="number" name="month" maxlength="2" class="form-control" id="inputZip" placeholder="DD">
      </div>
      <div class="form-group col-md-2">
        <label for="inputZip">Year</label>
        <input type="number" name="year" maxlength="4" class="form-control" id="inputZip" placeholder="YYYY">
      </div>

      <div class="form-group col-md-2">
        <label for="inputZip">Title</label>
        <input type="text" name="title" class="form-control" id="inputZip" placeholder="Customer Success">
      </div>
    </div>
    
    <div class="form-group">
      <div class="form-check">
        <input class="form-check-input" name="paid" type="checkbox" id="gridCheck">
        <label class="form-check-label" for="gridCheck">
          Did you paid for the event?
        </label>
      </div>
    </div>
    <button type="submit" class="btn btn-primary">Add Attendee</button>
  </form>
  <%- include('../partials/footer') %>